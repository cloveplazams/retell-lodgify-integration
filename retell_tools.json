[
    {
        "type": "function",
        "name": "check_availability",
        "description": "Check for property availability based on the number of guests and dates. Returns a list of suitable and available properties.",
        "parameters": {
            "type": "object",
            "properties": {
                "start_date": {
                    "type": "string",
                    "description": "The check-in date in YYYY-MM-DD format."
                },
                "end_date": {
                    "type": "string",
                    "description": "The check-out date in YYYY-MM-DD format."
                },
                "guests": {
                    "type": "integer",
                    "description": "The number of guests needing accommodation."
                }
            },
            "required": [
                "start_date",
                "end_date",
                "guests"
            ]
        }
    },
    {
        "type": "function",
        "name": "create_booking",
        "description": "Create a tentative booking for a specific property.",
        "parameters": {
            "type": "object",
            "properties": {
                "property_id": {
                    "type": "integer",
                    "description": "The ID of the property to book."
                },
                "guest_name": {
                    "type": "string",
                    "description": "Full name of the guest."
                },
                "guest_email": {
                    "type": "string",
                    "description": "Email address of the guest."
                },
                "start_date": {
                    "type": "string",
                    "description": "Check-in date (YYYY-MM-DD)."
                },
                "end_date": {
                    "type": "string",
                    "description": "Check-out date (YYYY-MM-DD)."
                }
            },
            "required": [
                "property_id",
                "guest_name",
                "guest_email",
                "start_date",
                "end_date"
            ]
        }
    }
]